{
  "name": "learnstorybook-design-system",
  "version": "0.1.2",
  "private": false,
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "polished": "^3.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "build": "BABEL_ENV=production babel src -d dist",
    "prepare": "npm run build",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "build-storybook-docs": "build-storybook -s public --docs",
    "changelog": "auto changelog && git reset HEAD^ && $EDITOR ./CHANGELOG.md && git commit -am ‘Updated changelog for `auto version` [skip ci]’",
    "version": "npm version `auto version` -m ‘Bump version to %s [skip ci]’",
    "publish": "npm publish && git push --follow-tags",
    "release": "npm run changelog && npm run version && npm run publish && auto release"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.2.0-rc.2",
    "@storybook/addon-actions": "^5.2.0-rc.2",
    "@storybook/addon-docs": "^5.2.0-rc.2",
    "@storybook/addon-knobs": "^5.2.0-rc.2",
    "@storybook/addon-links": "^5.2.0-rc.2",
    "@storybook/addon-storysource": "^5.2.0-rc.2",
    "@storybook/addons": "^5.2.0-rc.2",
    "@storybook/react": "^5.2.0-rc.2",
    "auto": "^7.4.3",
    "prettier": "^1.18.2",
    "storybook-chromatic": "^2.2.2"
  },
  "description": "Learn Storybook design system",
  "main": "dist/index.js",
  "repository": "https://github.com/chromaui/learnstorybook-design-system.git",
  "author": "Tom Coleman <tom@thesnail.org>",
  "license": "MIT"
}
